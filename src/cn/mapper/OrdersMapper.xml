<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mapper.OrdersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.entity.Orders">
        <id column="id" property="id" />
        <result column="uid" property="uid" />
        <result column="cid" property="cid" />
        <result column="sid" property="sid" />
        <result column="bsite" property="bsite" />
        <result column="esite" property="esite" />
        <result column="price" property="price" />
        <result column="btime" property="btime" />
        <result column="etime" property="etime" />
        <result column="optime" property="optime" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uid, cid, sid, bsite, esite, price, btime, etime, optime, status
    </sql>

</mapper>
